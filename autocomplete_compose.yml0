version: "3.0"
services:
  dbpedia-lookup-autocompose:
    env_file: .env_autocomplete
    build: .
    restart: always
    ports:
      - 9274:8080
    volumes:
      - /appli/dbpedia_proj-data/lookup/autocomplete/app-config.yml:/root/app-config.yml
      - /appli/dbpedia_proj-data/lookup/template.xsl:/root/template.xsl
      - /appli/dbpedia_proj-data/lookup/autocomplete/index:/root/index/
      - /appli/dbpedia_proj-data/lookup/last_data/:/root/data
      - /appli/dbpedia_proj-data/lookup/autocomplete/tdb:/root/tdb                                     

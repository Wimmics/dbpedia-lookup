version: "3.0"
services:
  dbpedia-lookup-search:
     env_file: .env_search
     build: .
     restart: always
     ports:
          - 9273:8080
     volumes: 
          - /appli/dbpedia_proj-data/lookup/search/app-config.yml:/root/app-config.yml
          - /appli/dbpedia_proj-data/lookup/template.xsl:/root/template.xsl
          - /appli/dbpedia_proj-data/lookup/search/index:/root/index/
          - /appli/dbpedia_proj-data/lookup/last_data/:/root/data/
          - /appli/dbpedia_proj-data/lookup/search/tdb:/root/tdb
